    "version": "2.0.2",
    "type": "project",
    "require": {
        "php": "^8.0",
        "ext-json": "*",
        "chindit/collection": "^1.0.8",
        "symfony/console": "^6.1",
        "symfony/http-client": "^6.1"
    },
    "require-dev": {
        "phpstan/phpstan": "^1.8.2",
        "symfony/test-pack": "^1.0",
        "symfony/maker-bundle": "^1.45"
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "David Lumaye",
            "email": "littletiger58@gmail.com"
        }
    ],
    "autoload": {
