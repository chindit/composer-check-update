    "version": "1.0.5",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "ext-json": "*",
        "symfony/console": "^5.2",
        "symfony/http-client": "^5.2"
    },
    "require-dev": {
        "symfony/test-pack": "^1.0",
        "symfony/maker-bundle": "^1.28"
    },
    "license": "GPL-3.0-only",
    "authors": [
        {
            "name": "David Lumaye",
            "email": "littletiger58@gmail.com"
        }
